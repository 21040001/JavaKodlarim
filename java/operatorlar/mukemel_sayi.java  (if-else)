//if va else 
public class mukemel_sayi { 
    public static void main(String[] args) 
        int a=2
        int b=3
        if (a > b){
            System.out.println("kirilgan a sonimiz b dan kattadir");
        }else{
        System.out.println("kirilgan a sonimiz b dan kichkinadir");
        }


    }
}
